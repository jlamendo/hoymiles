{
  "name": "HoyMiles Solar Data Gateway Add-on",
  "version": "0.20",
  "slug": "hoymiles_solardata",
  "description": "Read Hoymiles Gateway Solar Data.",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/dmslabsbr/hoymiles",
