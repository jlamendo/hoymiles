  "webui": "[PROTO:use_ssl]://[HOST]:[PORT:5000]/",
  "ingress": true,
  "ingress_port": 5000,
  "host_network": true,
  "panel_icon": "mdi:solar-power",
  "panel_title": "HoyMiles Solar Data Gateway",
  "panel_admin": false,
