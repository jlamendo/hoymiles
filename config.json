    "MQTT_USER": "MQTT_USER",
    "MQTT_PASSWORD": "MQTT_PASSWORD",
    "DEVELOPERS_MODE": false
  },
  "schema": {
    "HOYMILES_USER": "str",
    "HOYMILES_PASSWORD": "password",
    "HOYMILES_PLANT_ID": "str",
    "MQTT_HOST": "str",
    "MQTT_USER": "str",
    "MQTT_PASSWORD": "password",
    "DEVELOPERS_MODE": "bool"
  }
